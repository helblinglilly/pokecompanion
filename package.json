  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:app",
    "build:assets": "node scripts/socialpreviews/socialGenerator.js",
    "build:app": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
